
@model System.Web.Mvc.HandleErrorInfo



<!DOCTYPE html>
<html class=" ">
<head>

    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="utf-8" />
    <title>Ultra Admin : 404 Error Page</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta content="" name="description" />
    <meta content="" name="author" />

    <link rel="shortcut icon" href="/Content/default/images/favicon.png" type="image/x-icon" />    <!-- Favicon -->
    <link rel="apple-touch-icon-precomposed" href="/Content/default/images/apple-touch-icon-57-precomposed.png">	<!-- For iPhone -->
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/Content/default/images/apple-touch-icon-114-precomposed.png">    <!-- For iPhone 4 Retina display -->
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/Content/default/images/apple-touch-icon-72-precomposed.png">    <!-- For iPad -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/Content/default/images/apple-touch-icon-144-precomposed.png">    <!-- For iPad Retina display -->

    @Styles.Render("~/Content/default/css")

</head>
<!-- END HEAD -->
<body class=" ">

    <div class="col-lg-12">
        <section class="box nobox">
            <div class="content-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">

                        <h1 class="page_error_code text-primary">403</h1>
                        <h1 class="page_error_info">Oops! Forbidden.</h1>

                        <div class="col-md-6 col-sm-6 col-xs-8 col-md-offset-3 col-sm-offset-3 col-xs-offset-2">
                            <form action="javascript:;" method="post" class="page_error_search">
                                <div class="text-center page_error_btn">
                                    <a class="btn btn-primary btn-lg" href='/'><i class='fa fa-location-arrow'></i> &nbsp; Back to Home</a>
                                </div>
                            </form>
                        </div>

                    </div>
                </div>
            </div>
        </section>
    </div>


</body>
</html>




